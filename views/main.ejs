<script type="text/x-tmpl" id="tmpl-tasks">
<div class="">
    <div class="page-title">
        <div class="title_left">
            <h3><%=lang.task_center%></h3>
        </div>
    </div>
    <div class="clearfix"></div>

    <div class="row" style="height:520px;">
        <div class="col-md-6 col-sm-6 col-xs-12" style="width: 100%">
            <div class="x_panel">
                <div class="x_title">
                    <h2><%=lang.task_list%></h2>
                    <div class="clearfix"></div>
                </div>
                <div class="x_content">
                    <div class="" role="tabpanel" data-example-id="togglable-tabs">
                        <ul id="myTab" class="nav nav-tabs bar_tabs" role="tablist">
                          <li role="presentation" class="active">
                            <a href="#tab_content1" id="home-tab" role="tab" data-toggle="tab" aria-expanded="true"><%=lang.todo_list%></a>
                          </li>
                          <li role="presentation" class=""><a href="#tab_content2" role="tab" id="profile-tab" aria-expanded="false" style="cursor:no-drop;color:darkgray;"><%=lang.task_list_history%></a>
                          </li>
                        </ul>
                    <div id="myTabContent" class="tab-content">
                      <div role="tabpanel" class="tab-pane fade active in" id="tab_content1" aria-labelledby="home-tab">
                        <table id="table-tasks" class="table table-striped jambo_table" border="1">
                            <thead>
                            <tr>
                                <th class="column-title middle" width="10%"><%=lang.bill_number%></th>
                                <th class="column-title" width="10%"><%=lang.bill_attribute%></th>
                                <th class="column-title" width="10%"><%=lang.bill_type%></th>
                                <th class="column-title middle" width="10%"><%=lang.bill_amount_add_currency%></th>
                                <th class="column-title middle" width="10%"><%=lang.payer%></th>
                                <th class="column-title middle" width="10%"><%=lang.accepter%></th>
                                <th class="column-title middle" width="10%"><%=lang.cash_person%></th>
                                <th class="column-title middle" width="10%"><%=lang.bill_expire_date%></th>
                                <th class="column-title middle" width="10%"><%=lang.cash_date%></th>
                                <th class="column-title middle" width="10%"><%=lang.operation%></th>
                            </tr>
                            </thead>
                            <tbody>
                            {% for (var i = 0; i < o.tasks.length; i++) { %}
                                <tr data-bill_no="{%=o.tasks[i].bill_no%}"
                                    data-current_step="{%=o.tasks[i].current_step.id%}">
                                    <td class="center middle" name="bill_no">{%=o.tasks[i].bill_no%}</td>
                                    <td class="center middle">{% if (o.tasks[i].bill_attr == 0) { %}<%=lang.bill_attribute_paper%>{% } else { %}<%=lang.bill_attribute_electronic%>{% } %}</td>
                                    <td class="center middle">{% if (o.tasks[i].bill_type == 0) { %}<%=lang.bank_bill%>{% } else { %}<%=lang.business_bill%>{% } %}</td>
                                    <td class="right middle">{%=o.tasks[i].face_amount%}</td>
                                    <td class="middle">{%=o.tasks[i].issuer_name%}</td>
                                    <td class="middle">{%=o.tasks[i].custodian_name%}</td>
                                    <td class="middle">{%=o.tasks[i].acceptor_name%}</td>
                                    <td class="center middle">{%=o.tasks[i].due_date%}</td>
                                    <td class="center middle">{%=o.tasks[i].accept_date%}</td>
                                    <td class="center noselect">
                                        {% if (o.tasks[i].current_step.actions) { %}
                                        <button type="submit" class="btn btn-success" data-next="{%=o.tasks[i].current_step.actions.next.id%}">{%=o.tasks[i].current_step.actions.next.action%}</button>
                                        {% if (o.tasks[i].current_step.actions.back) { %}
                                        <button type="submit" class="btn btn-primary">{%=o.tasks[i].current_step.actions.back.action%}</button>
                                        {% }} else { %} <%=lang.done%> {% } %}
                                    </td>
                                </tr>
                            {% } %}
                            </tbody>
                        </table>
                      </div>
                      <div role="tabpanel" class="tab-pane fade" id="tab_content2" aria-labelledby="profile-tab">
                        <p>TO BE DONE.</p>
                      </div>
                    </div>
                  </div>
                </div>
            </div>
        </div>
