<script>
    function loadTasks() {
        var token = $('body').attr('data-token');

        var option = {
            type:"GET",
            url:"/api/task/list",
            dataType:"json",
            data:{
            },
            beforeSend: function(xhr){
                xhr.setRequestHeader('token', token);
            },
            success:function(res){
                if (res.result) {
                    console.log(res);
                } else {
                    console.log(res);
                }
            },
            error:function(err){
                alert(err);
            }
        };
        $.ajax(option);
    }
</script>

