                <input type="radio" class="icheck" value="0" name="radio-trans_enable" {% if (o.trans_enable == 0) { %}checked{% } %}
                    readonly> <%=lang.assignment_sign_ok%></label>
            &nbsp;
            <label><input type="radio" class="icheck" value="1" name="radio-trans_enable" {% if (o.trans_enable == 1) { %}checked{% } %}
