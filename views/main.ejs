<script type="text/x-tmpl" id="tmpl-bill_detail">
<div class="row">
    <div class="col-md-12">
        <form class="form-horizontal" data-parsley-validate>
        {% if (o.current_step.actions.next.id == '03') { %}
            <div class="form-group">
              <label class="control-label col-md-3 col-sm-3 col-xs-12" for="text-saler_party"><%=lang.seller_organization%> <span class="required">*</span></label>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <input type="text" id="text-saler_party" class="form-control col-md-7 col-xs-12" value="P1"
                    readonly/>
              </div>
            </div>
            <div class="form-group">
              <label class="control-label col-md-3 col-sm-3 col-xs-12" for="text-buyin_party"><%=lang.buyer_organization%> <span class="required">*</span></label>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <input type="text" id="text-buyin_party" class="form-control col-md-7 col-xs-12"  value="P2"
                    readonly/>
              </div>
            </div>
            <div class="form-group">
              <label class="control-label col-md-3 col-sm-3 col-xs-12" for="text-deal_amount"><%=lang.transaction_amount%> <span class="required">*</span></label>
              <div class="col-md-6 col-sm-6 col-xs-12">
              <!-- 默认 票据利息是 ５％,  交易金额 = 票面金额 * (1 - 票据利息),  也就是 票面金额10000元的话，实际入手的交易金额为9500元 -->
                <input type="text" id="text-deal_amount" class="form-control col-md-7 col-xs-12" value="{%= o.face_amount * 0.95 %}"
                    data-parsley-trigger="input change" data-parsley-range="[1, 3000000]" required/>
              </div>
            </div>
        {% } %}
